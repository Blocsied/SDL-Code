<ui:ContentDialog x:Class="Turtlz_Launcher.GamePathPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Turtlz_Launcher"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Game Path"
      xmlns:ui="http://schemas.modernwpf.com/2019">

    <Grid>
        <StackPanel>
            <TextBox x:Name="txtMCpath"/>
            <Button x:Name="btnOK" Click="btnOK_Click" Content="ok"/>
            <Button x:Name="btnDefalt" Click="btnDefalt_Click" Content="Default"/>
        </StackPanel>
    </Grid>
</ui:ContentDialog>
